<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd//hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="datos.Profesor" table="profesor">
		 <id column="idProfesor" name="idProfesor">
        <generator class="foreign">
			    <param name="property">direccion</param>
			    
			    </generator>
   
		</id>
		<property column="nombre" name="nombre" type="string"></property>
		<property column="apellido" name="apellido" type="string"></property>
		<one-to-one name="direccion" class="datos.Direccion"></one-to-one>
		
	</class>
</hibernate-mapping>