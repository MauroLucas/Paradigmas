<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd//hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="datos.Observador" table="observador">
<id column="idObservador" name="idObservador">
<generator class="identity"/>
</id>
<property column="observador" name="observador" type="string"></property>
<property column="institucion" name="institucion" type="string"></property>

</class>
</hibernate-mapping>
