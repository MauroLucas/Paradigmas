<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd//hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="datos.Observacion" table="observacion">
<id column="idObservacion" name="idObservacion">
<generator class="identity"/>
</id>
<property column="ordenPrioridad" name="ordenPrioridad" type="int"></property>
<property column="fechaHoraUtilizacion" name="fechaHoraUtilizacion" type="calendar"></property>
<property column="fechaHoraInicioExposicion" name="fechaHoraInicioExposicion" type="calendar"></property>
<property column="fechaHoraFinExposicion" name="fechaHoraFinExposicion" type="calendar"></property>
<property column="tiempoExposicion" name="tiempoExposicion" type="int"></property>

<many-to-one name="observador" class="datos.Observador" column="idObservador"></many-to-one>
<many-to-one name="objetoCeleste" class="datos.ObjetoCeleste" column="idObjetoCeleste"></many-to-one>
</class>



</hibernate-mapping>